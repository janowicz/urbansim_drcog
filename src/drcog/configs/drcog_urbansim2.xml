<opus_project>
  <xml_version>2.0</xml_version>
  <general>
    <project_name type="string">drcog2</project_name>
    <description type="string">DRCOG UrbanSim2.0 re-implementation</description>
    <available_datasets type="list">['household','establishment','building']</available_datasets>
    <spatial_datasets type="list">['parcel','zone','county','region']</spatial_datasets>
  </general>
  <scenario_manager>
    <scenario executable="True" name="run_simulation" type="scenario">
      <model_system type="string">opus_core.model_coordinators.model_system</model_system>
      <base_year type="integer">2010</base_year>
      <years_to_run config_name="years" type="tuple">
        <firstyear type="integer">2011</firstyear>
        <lastyear type="integer">2011</lastyear>
      </years_to_run>
      <models_to_run config_name="models" type="selectable_list">
        <selectable name="urbansim2_1" return_value="urbansim2" type="selectable">True</selectable>
      </models_to_run>
      <creating_baseyear_cache_configuration type="class">
        <class_name hidden="FALSE" type="string">CreatingBaseyearCacheConfiguration</class_name>
        <class_module hidden="FALSE" type="string">opus_core.configurations.creating_baseyear_cache_configuration</class_module>
        <argument config_name="cache_directory_root" name="scenario_runs_directory" parser_action="prefix_with_opus_data_path" type="directory">drcog2/runs</argument>
        <argument name="existing_cache_to_copy" parser_action="prefix_with_opus_core" type="directory">data/test_cache/</argument>
        <argument name="cache_from_database" type="boolean">False</argument>
      </creating_baseyear_cache_configuration>
      <dataset_pool_configuration type="class">
        <class_name hidden="FALSE" type="string">DatasetPoolConfiguration</class_name>
        <class_module hidden="FALSE" type="string">opus_core.configurations.dataset_pool_configuration</class_module>
        <argument name="package_order" type="list">['opus_core']</argument>
      </dataset_pool_configuration>
      <datasets_to_preload parser_action="list_to_dictionary" type="selectable_list">
</datasets_to_preload>
    </scenario>
  </scenario_manager>
  <model_manager>
    <models config_name="model_system" hidden="False" name="Models" type="dictionary">
      <model name="urbansim2" type="model">
        <structure type="dictionary">
          <import name="import" type="dictionary">
            <class_module type="string">drcog.models.urbansim2</class_module>
            <class_name type="string">Urbansim2</class_name>
          </import>
          <init type="dictionary">
            <name name="name" type="string">Urbansim2</name>
            <argument name="scenario" type="string">'baseline'</argument>
          </init>
          <run type="dictionary">
            <argument name="name" type="string">'baseline'</argument>
            <argument name="export_buildings_to_urbancanvas" type="boolean">False</argument>
            <argument name="base_year" type="integer">2010</argument>
            <argument name="forecast_year" type="integer">2012</argument>
            <argument name="fixed_seed" type="boolean">True</argument>
            <argument name="random_seed" type="integer">1</argument>
            <argument name="export_indicators" type="boolean">True</argument>
            <argument name="indicator_output_directory" type="string">'c:/opus/data/drcog2/runs'</argument>
            <argument name="core_components_to_run" type="dictionary">
              <key name="HLCM" type="boolean">True</key>
              <key name="ELCM" type="boolean">True</key>
              <key name="Price" type="boolean">True</key>
              <key name="Developer" type="boolean">True</key>
            </argument>
            <argument name="household_transition" type="dictionary">
              <key name="Enabled" type="boolean">True</key>
              <key name="control_totals_table" type="string">annual_household_control_totals</key>
              <key name="scaling_factor" type="float">1</key>
            </argument>
            <argument name="household_relocation" type="dictionary">
              <key name="Enabled" type="boolean">True</key>
              <key name="relocation_rates_table" type="string">annual_household_relocation_rates</key>
              <key name="scaling_factor" type="float">1</key>
            </argument>
            <argument name="employment_transition" type="dictionary">
              <key name="Enabled" type="boolean">True</key>
              <key name="control_totals_table" type="string">annual_employment_control_totals</key>
              <key name="scaling_factor" type="float">1</key>
            </argument>
            <argument name="elcm_configuration" type="dictionary">
              <key name="building_sqft_per_job_table" type="string">building_sqft_per_job</key>
              <key name="scaling_factor" type="float">1</key>
            </argument>
            <argument name="developer_configuration" type="dictionary">
              <key name="Proforma" type="boolean">False</key>
              <key name="vacancy_rates_table" type="string">target_vacancies</key>
              <key name="vacancy_scaling_factor" type="float">1</key>
            </argument>
          </run>
        </structure>
      </model>
    </models>
  </model_manager>
  <results_manager>
    <simulation_runs name="Simulation Runs" setexpanded="True" type="all_source_data">
    </simulation_runs>
  </results_manager>
</opus_project>
